{
  "name": "googlescraper",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cypress:open": "cypress open --config-file=cypress.config.ts",
    "cypress:run": "cypress run --config-file=cypress.config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./ormconfig.ts",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./ormconfig.ts schema:drop",
    "db:create": "ts-node ./scripts/create-database.ts",
    "db:migrate": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js --config ./ormconfig.ts migration:run",
    "tailwind:build": "tailwindcss -i ./src/public/styles/styles.css -o ./dist/public/tailwindstyles.css",
    "tailwind:watch": "tailwindcss -i ./src/public/styles/styles.css -o ./dist/public/tailwindstyles.css --watch",
    "styling:compile": "npm run tailwind:build && gulp",
    "styling:gulp": "gulp"
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.3.3",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "autoprefixer": "^10.4.7",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.4",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express-session": "^1.17.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "hbs": "^4.2.0",
    "joi": "^17.6.0",
    "node-sass": "^7.0.1",
    "papaparse": "^5.3.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.0.0",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/gulp": "^4.0.9",
    "@types/jest": "27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/papaparse": "^5.3.2",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cypress": "^10.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "postcss": "^8.4.14",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
